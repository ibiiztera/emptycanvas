public class PlacementObjet {
    private Representable r;
    private MODObjet mo;
    public PlacementObjet(Representable r, MODObjet mo)
    {
        this.r = r;
        this.mo = mo;
    }
    public Representable RenPlace()
    {
        return r.place(mo);
    }
